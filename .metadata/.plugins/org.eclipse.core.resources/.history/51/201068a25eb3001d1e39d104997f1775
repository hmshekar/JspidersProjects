class Hotel{
	public Object orderFood(int choice) {
		if(choice==1) {
			return new Biriyani();
		}
		else if(choice==2) {
			return new CurdRice();
		}
		else if(choice==3) {
			return new EggFriedClass();
		}
		else
			return null;
	}
