package com;

class Food{
	
}

class Biriyani extends Food{
	
}

class CurdRice extends Food{
	
}

class EggFriedClass extends Food{
	
}

class Hotel{
	public Food orderFood(int choice) {
		if(choice==1) {
			return new Biriyani();
		}
		else if(choice==2) {
			return new CurdRice();
		}
		else if(choice==3) {
			return new EggFriedClass();
		}
		else
			return null;
	}
}
public class Solution {
public static void main(String[] args) {
	
}
}
